@model IEnumerable<TPBiblioteca.Models.Consulta>

@{
    ViewBag.Title = "Consulta";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Nueva Consulta", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.TituloCon)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Consulta1)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.TituloCon)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Consulta1)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.idConsulta }) |
            @Html.ActionLink("Details", "Details", new { id=item.idConsulta }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.idConsulta })
        </td>
    </tr>
}

</table>
